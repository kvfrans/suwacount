{
  "manifest_version": 2,

  "name": "Youtube Counter",
  "description": "you can see how many times you watched a video. get off youtube!!!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["suwa3.png"],
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "<all_urls>",
    "alarms"
  ]
}
